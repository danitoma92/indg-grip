{
    "name": "e2e",
    "version": "2.0.0",
    "description": "Playwright tests",
    "main": "index.js",
    "scripts": {
        "test": "npx playwright test tests/e2e/mp4.spec.ts --project=chromium --trace=off",
        "chromium": "npx playwright test --project=chromium --trace=off",
        "build": "docker build -t playwright-docker .&& npm run run:docker",
        "run:docker": "docker run -it playwright-docker:latest npm run test",
        "test-html-report": "playwright test --reporter=html",
        "test-json-report": "PLAYWRIGHT_JSON_OUTPUT_NAME=results.json playwright test --browser=chromium --reporter=json"
    },
    "keywords": [
        "playwright",
        "test"
    ],
    "author": "Dani Toma",
    "license": "ISC",
    "devDependencies": {
        "@aws-sdk/client-s3": "^3.288.0",
        "@playwright/test": "1.35.1",
        "@types/adm-zip": "^0.5.0",
        "@types/ffprobe-static": "^2.0.1",
        "@types/fluent-ffmpeg": "^2.1.20",
        "@types/glob": "^8.1.0",
        "@types/node-fetch": "2.6.3",
        "@types/pixelmatch": "^5.2.4",
        "@types/pngjs": "^6.0.1",
        "@types/streamifier": "^0.1.0",
        "@types/uuid": "9.0.7",
        "chalk": "4.1.2",
        "lighthouse": "9.6.8",
        "monocart-reporter": "^1.6.18",
        "node-fetch": "^2.6.9",
        "playwright": "^1.43.1",
        "playwright-lighthouse": "2.2.2",
        "pngjs": "^7.0.0",
        "streamifier": "^0.1.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@apollo/client": "3.8.8",
        "@aws-sdk/types": "^3.272.0",
        "@types/node": "^18.14.6",
        "adm-zip": "^0.5.10",
        "ansi-colors": "^4.1.3",
        "apollo-link-batch-http": "1.2.14",
        "apollo-link-context": "1.0.20",
        "aws-sdk": "^2.1328.0",
        "cli-progress": "^3.12.0",
        "dotenv": "^16.0.3",
        "ffmpeg-static": "^5.1.0",
        "ffmpeg-static-electron": "^2.0.3",
        "ffprobe-static": "^3.1.0",
        "fluent-ffmpeg": "^2.1.2",
